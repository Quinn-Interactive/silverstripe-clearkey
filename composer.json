{
  "name": "quinninteractive/silverstripe-clearkey",
  "description": "Silverstripe module to manage partial-cache invalidation",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "silverstripe",
    "cache",
    "caching",
    "key",
    "invalidation",
    "flush"
  ],
  "license": "BSD-3-Clause",
  "authors": [
      {
          "name": "Quinn Interactive, Inc.",
          "email": "support+clearkey@quinn.com",
          "homepage": "https://quinn.com/",
          "role": "Developer"
      }
  ],
  "require": {
    "php": "^8.0 || ^7.4",
    "silverstripe/vendor-plugin": "^1.0",
    "silverstripe/framework": "^4.0",
    "silverstripe/versioned": "^1.0",
    "silverstripe/admin": "^1.0"
  },
  "autoload": {
      "psr-4": {
          "QuinnInteractive\\ClearKey\\": "src/"
      }
  },
  "extra": {
    "expose": [
      "client/images"
    ]
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    }
  },
  "require-dev": {
    "vimeo/psalm": "^4.29"
  }
}
